#include<stdio.h>
 int main()
 {
   int n,i,a,c;
   printf("Enter the number:  ");
   scanf("%d",&n);
   if (n==1)
   {
    printf("The number is neither prime nor composite");
   }
    else
    {
     for(i=2; i< (n-1) ; ++i)
     {
      a= n%i;
      if (a==0)
      {
        c=0;
        break;
       }
       else
       {
        c=2;
       }
     }
   if(c==0)
   {
   printf("\nThe number is composite");
   }
   else
   {
     printf("The number is prime");
   }

   }
    return 0;
 }
